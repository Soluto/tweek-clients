{
  "compileOnSave": true,
  "exclude": ["dist", "node_modules", "spec"],
  "compilerOptions": {
    "lib": ["dom", "es2015", "es2017.object", "ES2016.Array.Include"],
    "target": "es5",
    "allowSyntheticDefaultImports": true,
    "moduleResolution": "node",
    "strictNullChecks": true,
    "declaration": true,
    "sourceMap": true,
    "outDir": "./dist"
  }
}
