{
  "name": "tweek-local-cache",
  "version": "0.1.3",
  "description": "Local cache to be used with tweek-client",
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "dependencies": {
    "object.entries": "^1.0.4",
    "object.values": "^1.0.4",
    "tweek-client": "^0.1.2"
  },
  "devDependencies": {
    "@types/chai": "^4.0.4",
    "@types/chai-as-promised": "^7.1.0",
    "@types/mocha": "^2.2.36",
    "@types/node": "^8.0.28",
    "@types/sinon": "^2.3.4",
    "@types/sinon-chai": "^2.7.27",
    "axios": "^0.16.2",
    "chai": "^4.1.2",
    "chai-as-promised": "^7.1.1",
    "getenv": "^0.7.0",
    "isomorphic-fetch": "^2.2.1",
    "mocha": "^3.2.0",
    "simple-fake-server": "^0.3.3",
    "sinon": "^3.2.1",
    "sinon-chai": "^2.8.0",
    "ts-node": "^3.3.0",
    "typescript": "^2.1.4"
  },
  "scripts": {
    "dev": "tsc -w",
    "test": "yarn test:unit && yarn test:behavior",
    "test:behavior": "mocha --require spec/init-mocha.js --compilers ts:ts-node/register spec/behavior/*.spec.ts",
    "test:behavior-debug":
      "mocha --debug-brk --require spec/init-mocha.js --compilers ts:ts-node/register spec/behavior/*.spec.ts",
    "test:unit": "mocha --compilers ts:ts-node/register spec/unit/*.spec.ts",
    "build": "tsc"
  },
  "author": "Soluto",
  "license": "MIT"
}
